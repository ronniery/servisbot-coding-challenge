{
  "name": "src",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "test:unit": "vitest run --exclude \"**/*.e2e.test.tsx\"",
    "test:e2e": "playwright test",
    "test:coverage": "vitest run --coverage",
    "build": "tsc -b && vite build",
    "format": "prettier --check \"src/**/*.{ts,tsx}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.{ts,tsx}\" --fix",
    "docker:build": "docker build -t frontend -f Dockerfile ../..",
    "preview": "vite preview"
  },
  "dependencies": {
    "@packages/shared": "file:../../packages/shared",
    "axios": "^1.13.2",
    "axios-cache-interceptor": "^1.8.3",
    "class-validator": "^0.14.3",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/coverage-v8": "^4.0.15",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "prettier": "^3.7.4",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "npm:rolldown-vite@7.2.6",
    "vitest": "^4.0.15"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.6"
  }
}
